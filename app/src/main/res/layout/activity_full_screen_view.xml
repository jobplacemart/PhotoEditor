<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:layout_width="match_parent"
    n1:layout_height="match_parent">

    <LinearLayout
        n1:layout_width="match_parent"
        n1:layout_height="match_parent"
        n1:layout_gravity="center"
        n1:background="#80151515"
        n1:gravity="center">

        <com.jaydip.dropshadowforinsta.creation.TouchImageView
            n1:id="@+id/iv_image"
            n1:layout_width="match_parent"
            n1:layout_height="match_parent"
            n1:layout_gravity="center"
            n1:scaleType="fitCenter"
            n1:src="@mipmap/ic_launcher" />
    </LinearLayout>
</RelativeLayout>
